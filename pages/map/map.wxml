<map
  id="map"
  longitude="114.24779"
  latitude="22.71991"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
   polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 300px;">
</map>
<button bindtap="openActionSheet">显示操作菜单</button>
<button bindtap="phoneCall">拨打热线电话</button>
<button bindtap="addContact">添加到通讯录</button>
<button bindtap="setClipboard">将地址复制到剪贴板</button>
<button bindtap="getClipboard">将剪贴板的内容粘贴到地址栏</button>
<input
  placeholder="这是剪贴板的内容"
  value="{{ClipboardData}}"
></input>
<button open-type="getUserInfo" bindgetuserinfo="getUserInfo">获取用户信息</button>
<open-data type="userNickName"></open-data>